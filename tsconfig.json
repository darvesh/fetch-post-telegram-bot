{
	"compilerOptions": {
		"module": "CommonJS",
		"removeComments": true,
		"incremental": true,
		"preserveConstEnums": true,
		"outDir": "dist",
		"strict": true,
		"esModuleInterop": true,
		"target": "ES2019",
		"lib": ["ES2020"]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "test"]
}
